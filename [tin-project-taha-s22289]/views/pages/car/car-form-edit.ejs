  <h2>Edit A Rent</h2>


        <form class="form">
           
            <label for="model">Car Model:<abbr title="required" aria-label="required"></abbr></label>
            <input type="text" name="model" value="Audi: e-tron GT quattro" id="model" />
            <span id="errormodel" class="errors-text"></span>

            <label for="year">Car Year:<abbr title="required" aria-label="required"></abbr></label>
            <input type="date" name="year" value="2022-05-06" id="year"/>
            <span id="erroryear" class="errors-date"></span>

            <label for="colar">Car Colar:</label>
            <input type="text" name="colar" value="Blue" id="colar"/>
            <span id="errorcolar" class="errors-text"></span>

            <label for="status">Car Status:</label>
            <input type="text" name="status" value="New" id="status"/>
            <span id="errorstatus" class="errors-text"></span>

            <div>
              <input class="form-button-submit" type="submit" value=Add />
              <a class="form-button-cancel" href="car-list.html">Cancel</a>  
            </div>
</form>
